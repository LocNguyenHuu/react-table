{
  "name": "react-table",
  "version": "0.1.0",
  "description": "React Table is a simple, light table built on React. It makes no assumptions about your data and allows the user to define how the table reacts to pagination, filtering and sorting.",
  "scripts": {
    "server": "webpack-dev-server --progress --colours",
    "build": "npm run build-lib && npm run build-example",
    "build-lib": "rm -rf dist && mkdir dist && babel src --out-file 'dist/react-table.js'",
    "build-example": "webpack",
    "dev": "rm -rf dist && mkdir dist && babel src --watch --out-file 'dist/react-table.js'",
    "dev-example": "webpack-dev-server --progress --colors",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adi2412/react-table.git"
  },
  "keywords": [
    "React",
    "Table"
  ],
  "author": {
    "name": "Aditya Raisinghani",
    "email": "aditya.ajeet@gmail.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/adi2412/react-table/issues"
  },
  "homepage": "https://github.com/adi2412/react-table#readme",
  "dependencies": {
    "react": "^15.3.2"
  },
  "devDependencies": {
    "babel-cli": "6.18.0",
    "babel-core": "6.18.0",
    "babel-loader": "6.2.5",
    "babel-plugin-transform-object-rest-spread": "6.16.0",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-react": "6.16.0",
    "expose-loader": "0.7.1",
    "node-sass": "3.10.1",
    "react-dom": "15.3.2",
    "uglify-js": "1.3.5",
    "webpack": "1.13.2",
    "webpack-dev-server": "1.16.2"
  }
}
